{
  "atom-workspace": {
    "ctrl-alt-o": "vim-like-commands:toggle",
    "alt-h": "vim-like-commands:move-left",
    "alt-j": "vim-like-commands:move-down",
    "alt-k": "vim-like-commands:move-up",
    "alt-l": "vim-like-commands:move-right",

    "shift-alt-h": "vim-like-commands:select-left",
    "shift-alt-j": "vim-like-commands:select-down",
    "shift-alt-k": "vim-like-commands:select-up",
    "shift-alt-l": "vim-like-commands:select-right",

    "shift-alt-4": "vim-like-commands:move-to-end-of-line",
    "shift-alt-g": "vim-like-commands:move-to-bottom"
  }
}
